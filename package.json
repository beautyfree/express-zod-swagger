{
  "name": "express-zod-swagger",
  "version": "0.0.1",
  "description": "Express + Zod + Swagger",
  "keywords": [
    "express",
    "zod",
    "swagger",
    "oas3",
    "openapi"
  ],
  "author": "Alexey Elizarov",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/beautyfree/express-zod-swagger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beautyfree/express-zod-swagger.git"
  },
  "bugs": {
    "url": "https://github.com/beautyfree/express-zod-swagger/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^1.12.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "openapi3-ts": "^3.2.0",
    "swagger-ui-express": "^4.6.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-plugin-jest": "27.1.7",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "express": "^4.18.2",
    "openapi3-ts": "^3.2.0"
  }
}
